{
	"compilerOptions": {
		"baseUrl": "./",
		"target": "ESNext",
		"useDefineForClassFields": true,
		"module": "ESNext",
		"strict": true,
		"jsx": "preserve",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"lib": ["ESNext", "DOM"],
		"skipLibCheck": true,
		"noEmit": true,
		"moduleResolution": "Bundler",
		"paths": {
			"vuex":["./node_modules/vuex/types"],
			"@/*": [
				"src/*"
			]
		}
	},
	"include": ["src", "./typed-router.d.ts"],
	"references": [{
		"path": "./tsconfig.node.json"
	}]
}